{
  "name": "stencil-dev",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-dev/stencil-dev.esm.js",
  "exports": {
    ".": {
      "import": "./dist/stencil-dev/stencil-dev.esm.js",
      "require": "./dist/stencil-dev/stencil-dev.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs --watch",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "watch-storybook": "chokidar 'dist/**/*' -c 'pnpm storybook'"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@stencil/core": "^4.27.1",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/experimental-addon-test": "^8.6.4",
    "@storybook/test": "^8.6.4",
    "@storybook/web-components": "^8.6.4",
    "@storybook/web-components-vite": "^8.6.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "chokidar-cli": "^3.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lit": "^3.2.1",
    "puppeteer": "^24.3.0",
    "storybook": "^8.6.4",
    "vite": "^6.2.2"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.0.1+sha1.0e0a9c2d140ddf9aab730067eb7bcfb9e18bdee7"
}